{
  "name": "@ericnunes/frame-agent-server",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "description": "HTTP server for exposing GraphEngine execution graphs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "frame-agent-server": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "agent",
    "graph",
    "server",
    "fastify",
    "worker-threads"
  ],
  "author": "Eric Nunes",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericnunes/frame-agent-server.git"
  },
  "bugs": {
    "url": "https://github.com/ericnunes/frame-agent-server/issues"
  },
  "homepage": "https://github.com/ericnunes/frame-agent-server#readme",
  "peerDependencies": {
    "@ericnunes/frame-agent-sdk": "^0.0.6"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "fastify": "^5.7.2",
    "fastify-plugin": "^5.1.0",
    "pino": "^10.3.0",
    "pino-http": "^10.0.0"
  },
  "devDependencies": {
    "@ericnunes/frame-agent-sdk": "^0.0.6",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
